#
# Additional library definitions for the different platforms
#

LIB_WIN32_GLUT32	:= glut32.lib
LIB_WIN32		:= user32.lib gdi32.lib kernel32.lib advapi32.lib
LIB_UNIX_OPENGL		:= -lglut -lGL -lGLU -lXmu -lX11 -lXi
LIB_UNIX		:= -ltiff -lm
LIB_PS2			:= -lgraph -ldma -ldev -lpkt -lvu0 -lpad -lpc -lm -lstdc++

ifeq '$(PLATFORM)' 'win32'
	LIB_SYSTEM       := $(LIB_WIN32_GLUT32) $(LIB_WIN32)
	LIB_SYSTEM_DEBUG := $(LIB_WIN32_GLUT32) $(LIB_WIN32)
	LIB_SYSTEM_CHECK := $(LIB_WIN32_GLUT32) $(LIB_WIN32)
endif

ifeq '$(PLATFORM)' 'ps2'
	LIB_SYSTEM       := $(LIB_PS2)
	LIB_SYSTEM_DEBUG := $(LIB_PS2)
	LIB_SYSTEM_CHECK := $(LIB_PS2)
endif

ifeq '$(PLATFORM)' 'irix'
	LIB_SYSTEM       := $(LIB_UNIX_OPENGL) $(LIB_UNIX)
	LIB_SYSTEM_DEBUG := $(LIB_UNIX_OPENGL) $(LIB_UNIX)
	LIB_SYSTEM_CHECK := $(LIB_UNIX_OPENGL) $(LIB_UNIX)
endif

ifeq '$(PLATFORM)' 'linux'
	LIB_SYSTEM       := $(LIB_UNIX_OPENGL) $(LIB_UNIX)
	LIB_SYSTEM_DEBUG := $(LIB_UNIX_OPENGL) $(LIB_UNIX)
	LIB_SYSTEM_CHECK := $(LIB_UNIX_OPENGL) $(LIB_UNIX)
endif

ifeq '$(PLATFORM)' 'mips3'
	LIB_SYSTEM       := $(LIB_UNIX_OPENGL) $(LIB_UNIX)
	LIB_SYSTEM_DEBUG := $(LIB_UNIX_OPENGL) $(LIB_UNIX)
	LIB_SYSTEM_CHECK := $(LIB_UNIX_OPENGL) $(LIB_UNIX)
endif

ifeq '$(PLATFORM)' 'irix_o32'
        LIB_SYSTEM       := $(LIB_UNIX_OPENGL) $(LIB_UNIX)
        LIB_SYSTEM_DEBUG := $(LIB_UNIX_OPENGL) $(LIB_UNIX)
        LIB_SYSTEM_CHECK := $(LIB_UNIX_OPENGL) $(LIB_UNIX)
endif


